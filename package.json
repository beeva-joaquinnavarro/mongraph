    "documentation": "docco ./src/*.coffee",
    "doc": "groc ./src/*.coffee README.md",
    "benchmark": "coffee benchmark/benchmark.coffee"
  },
  "devDependencies": {
    "mocha": "~1.9",
    "expect.js": "~0.2",
    "should": "~1.2",
    "mongoose": "~3.6",
    "neo4j": "~0.2",
    "docco": "~0.6",
    "groc": "~0.4",
    "source-map-support": "~0.1.2",
    "mongoskin": "latest",
    "benchmark": "~1.0",
    "microtime": "*",
    "futures": "~2.3.1",
    "request": "~2.21.0"
  },
  "dependencies": {
    "underscore.string": "~2.3",
    "underscore": "~1.4",
    "join": "~2.3",
    "bson": "~0.1.8",
    "neo4jmapper": "*"
  },
  "licenses" : {
    "type" : "GPL"
  }
